
entity Facture {
	numero Integer unique required,
    dateDelivrance Instant,
    dateLimitePaiement Instant,
    consommation String,
    etat String,
    typeFacture TypeFacture
}
enum TypeFacture {
	SENELEC, SDE, SONATEL
}
entity Reclamation {
	objet String,
	libelle String
}

relationship OneToMany {
	User{facture} to Facture
}
relationship OneToMany {
	Facture{facture} to Reclamation{facture}
}
relationship OneToMany {
	User{reclamation} to Reclamation
}
